{
  "name": "profix",
  "displayName": "ProFix",
  "description": "AI-powered code analysis tool",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "profix.analyzeSelection",
        "title": "Run Selection Scan"
      },
      {
        "command": "profix.analyzeEdgeCases",
        "title": "Run Edge Cases Scan"
      }
    ],
    "submenus": [
      {
        "id": "profix.submenu",
        "label": "ProFix"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "profix.submenu",
          "group": "navigation"
        }
      ],
      "profix.submenu": [
        {
          "command": "profix.analyzeSelection"
        },
        {
          "command": "profix.analyzeEdgeCases"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.1.3"
  }
}